<%= render 'layouts/frontend/header' %>

<div class="sub-container">
    <div class="clearfloat">
        <%= render 'frontend/users/sidebar' %>

        <div class="sub-mainright">
            <div class="box-white min-h820">
                <div class="user-title-bar clearfloat">
                    <div class="t-left">基础资料</div>
                </div>
                <div class="padd30">
                    <div class="form-wrap">
                        <ul>
                            <li class="form-box">
                                <label class="form-label">真实姓名：</label>
                                <input type="text" id="" value="" size="40" class="form-input-style2 mr15" />
                            </li>
                            <li class="form-box">
                                <label class="form-label">生日：</label>
                                <input type="text" id="" value="" size="5" class="form-input-style2" />
                                <span class="mrl10">年</span>
                                <input type="text" id="" value="" size="5" class="form-input-style2" />
                                <span class="mrl10">月</span>
                                <input type="text" id="" value="" size="5" class="form-input-style2" />
                                <span class="mrl10">日</span>
                            </li>
                            <li class="form-box">
                                <label class="form-label">性别：</label>
                                <label>
                                    <input type="radio" name="" id="" value="" checked="checked" /><span class="mrl10">男</span></label>
                                <label>
                                    <input type="radio" name="" id="" value="" /><span class="mrl10">女</span></label>
                            </li>
                            <li class="form-box">
                                <input type="button" name="" id="" value="提  交" class="btn-red2 mt20" />
                            </li>
                        </ul>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <%= render 'layouts/frontend/footer' %>
</div>
